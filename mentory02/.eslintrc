{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
        "no-console": 2,
        "eqeqeq": 2,
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "prettier/prettier": ["error",{
          "endOfLine": "auto"}
        ]
    }
}